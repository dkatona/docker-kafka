{
  "consul": "[consul]",
  "logging": {
    "level": "[loglevel]",
    "format": "text",
    "output": "stdout"
  },
  "services": [
    {
      "name": "kafka",
      "port": 9092,
      "health": "/opt/kafka/bin/check.sh",
      "poll": 10,
      "ttl": 30
    }
  ],
  "backends": [
    {
      "name": "zookeeper",
      "poll": 10,
      "onChange": "/opt/kafka/bin/stop.sh"
    }
  ]
}
